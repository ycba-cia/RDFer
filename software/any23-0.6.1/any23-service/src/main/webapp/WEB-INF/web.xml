<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>any23 (Anything to Triples)</display-name>
  <servlet>
    <servlet-name>any23</servlet-name>
    <servlet-class>org.deri.any23.servlet.Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>redirect</servlet-name>
    <servlet-class>org.deri.any23.servlet.RedirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>any23</servlet-name>
    <url-pattern>/any23/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>redirect</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
